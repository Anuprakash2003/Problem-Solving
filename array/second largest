#include <stdio.h>
#include<limits.h>

int main()
{
    int arr[]={-123,12,45,78,90,-112,345};
    int max=INT_MIN;
    int smax=INT_MIN;
    int size=sizeof(arr)/sizeof(arr[0]);
    for(int i=0;i<size;i++)
    {
        if(arr[i]>smax)
        {
            if(arr[i]>max)
            {
                smax=max;
                max=arr[i];
            }
            else
            {
                smax=arr[i];
            }
        }
    }
    printf("%d",smax);

    return 0;
}
